{
  "name": "social-network",
  "version": "1.0.0",
  "description": "Training project for tt sunday school",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "NODE_ENV=development node src/index.js | bunyan",
    "prod": "NODE_ENV=production NPM_CONFIG_PRODUCTION=true node --optimize_for_size --max_old_space_size=500 --gc_interval=100 src/index.js | bunyan",
    "build": "webpack --config webpack.prod.js",
    "gen-data": "node generate-data.js",
    "prod-gen-data": "NODE_ENV=production node generate-data.js",
    "pm2": "pm2-runtime --env development --raw process.json | bunyan",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker-build": "docker-compose build --no-cache",
    "docker-start": "docker-compose up",
    "docker-clean": "docker-compose stop && docker-compose down --rmi local --volumes --remove-orphans",
    "eslint": "eslint --color ./",
    "postinstall": "cd front && npm i && cd ..",
    "heroku-postbuild": "npm run build && npm run prod-gen-data && npm run prod-gen-data",
    "precommit": "npm run eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://online-school.thumbtack.ru/aschukin/sunday-school.git"
  },
  "author": "Bitluck",
  "license": "MIT",
  "homepage": "https://tt-school-aschukin.herokuapp.com/",
  "dependencies": {
    "babel-core": "^6.26.0",
    "bcrypt": "^1.0.3",
    "bunyan": "^1.8.12",
    "fakerator": "^0.3.0",
    "file-type": "^7.6.0",
    "formidable": "^1.2.1",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-generic-session-sequelize": "^1.0.9",
    "koa-passport": "^4.0.1",
    "koa-router": "^7.4.0",
    "koa-session": "^5.8.1",
    "koa-static": "^4.0.2",
    "nconf": "^0.10.0",
    "npm": "^5.7.1",
    "passport-local": "^1.0.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "read-chunk": "^2.1.0",
    "sequelize": "^4.33.4",
    "useragent": "^2.3.0"
  },
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.17.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "husky": "^0.14.3",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.7.2",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.0",
    "postcss-reporter": "^5.0.0",
    "postcss-url": "^7.3.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^3.11.0",
    "webpack-cli": "^2.0.15"
  },
  "engines": {
    "node": "8.9.0",
    "npm": "5.5.1"
  }
}
